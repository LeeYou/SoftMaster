<SOUI name="mainWindow" title="@string/title" width="1014" height="670"  margin="20,5,5,5"  resizable="0" 
appWnd="1" translucent="1" >
  <root skin="_skin.shadow">
    <window pos="5,3,-5,-7" colorBkgnd="#009a61" catch="1" name="main_center">
			<caption pos="0,0,-0,80" >
				<icon pos="10,8" src="ICON_LOGO:16"/>
				<text pos="[5,8" colorText="#FFFFFF">软件管家</text>
				<imgbtn name="btn_close" skin="_skin.btn.close" pos="-35,0" tip="close" animate="1"/>
				<imgbtn name="btn_min" skin="_skin.btn.min"  pos="-62,0" animate="1" />
				<imgbtn name="btn_menu" skin="_skin.btn.menu"  pos="-89,0" animate="1" />
				<imgbtn name="btn_wis" skin="_skin.btn.wis" pos="-124,0" animate="1" />
			</caption>
			<window pos="0,28,-0,-0" >
				<window pos="0,0,-0,35"  >
					<mainTabBtn name="btn_softhouse" pos="200,0" 	skin="_skin.tabbar.status" icon="_skin.tabbar.download"  selected="1" colorText="#FFFFFF" colorTextPush="#242424">radio 1</mainTabBtn>
					<mainTabBtn name="btn_upgrade" pos="[2,{0"  	skin="_skin.tabbar.status" icon="_skin.tabbar.download"  selected="0" colorText="#FFFFFF" colorTextPush="#242424">radio 2</mainTabBtn>
					<mainTabBtn name="btn_unstall" pos="[2,{0"  	skin="_skin.tabbar.status" icon="_skin.tabbar.download"  selected="0" colorText="#FFFFFF" colorTextPush="#242424">radio 3</mainTabBtn>
					<mainTabBtn name="btn_download"  pos="[2,{0"  	skin="_skin.tabbar.status" icon="_skin.tabbar.download"  selected="0" colorText="#FFFFFF" colorTextPush="#242424">radio 4</mainTabBtn>
					
					<window pos="[20,{0" width="190" height="30" skin="_skin.searchbar.edit" >						
					  <edit pos="0,0,-35,-0" margin="0" mouseRelay="1" cueText="请输入想搜索的软件" colorText="#81CDB3" />
					  <imgbtn pos="-35,0" skin="_skin.searchbar.search" mouseRelay="1"  cursor="hand"/>			
					</window>
				</window>
				<window pos="0,35,-0,-30"  colorBkgnd="#FFFFFF">
			
					<window name="wnd_softhouse" pos="0,0,-0,-0" >
					<include src="layout:XML_PAGE_SOFTHOUSE" />
					</window>
					
					<window name="wnd_upgrade" pos="0,0,-0,-0" show="0">
					<include src="layout:XML_PAGE_UPGRADE" />
					</window>
					
					<window name="wnd_unstall" pos="0,0,-0,-0" show="0">
					<include src="layout:XML_PAGE_UNSTALL" />
					</window>
					
					<window name="wnd_download" pos="0,0,-0,-0" show="0">
					<include src="layout:XML_PAGE_DOWNLOAD" />
					</window>

				</window>
				<window pos="0,-30,-0,-0"  colorBkgnd="#009a61">
					<text pos="10,10" font="face:宋体,size:13" colorText="#FFFFFF">软件测试软件测试</text>
				</window>
		
			</window>
    </window>
  </root>
</SOUI>
